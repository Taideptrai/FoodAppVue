iner>\n</template>\n\n<script>\nexport default {\n    name: 'MealRecipes',\n    computed: {\n        recipes() {\n            return this.$store.state.recipes;\n        },\n        isAuthenticated() {\n            return this.$store.getters.isAuthenticated;\n        }\n    },\n    methods: {\n    orderRecipe(item) {\n        if (this.isAuthenticated) {\n            this.$store.dispatch('addRecipe', item);\n        } else {\n            this.$router.push('/Signin');\n        }\n    }\n}\n};\n\n</script>\n\n<style scoped>\n</style>"]}]}